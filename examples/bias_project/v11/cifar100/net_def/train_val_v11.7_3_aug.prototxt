  param { lr_mult: 1 decay_mult: 0 }
  param { lr_mult: 1 decay_mult: 0 }
  bn_param {
    slope_filler { type: "constant" value: 1 }
    bias_filler { type: "constant" value: 0 } } }
layer { name: "relu2_3_before" type: "ReLU" bottom: "conv2_3_bn" top: "conv2_3_bn" }
layer { name: "bias2_3_1" type: "Bias" bottom: "conv2_3_bn" top: "bias2_3_1"
  param { lr_mult: 2 decay_mult: 0 }
  bias_param { filler { type: "constant" value: 0 } channel_shared: false } }
layer { name: "bias2_3_2" type: "Bias" bottom: "conv2_3_bn" top: "bias2_3_2"
  param { lr_mult: 2 decay_mult: 0 }
  bias_param { filler { type: "constant" value: 0 } channel_shared: false } }
layer { name: "bias2_3_3" type: "Bias" bottom: "conv2_3_bn" top: "bias2_3_3"
  param { lr_mult: 2 decay_mult: 0 }
  bias_param { filler { type: "constant" value: 0 } channel_shared: false } }  
layer { name: "bias2_3_4" type: "Bias" bottom: "conv2_3_bn" top: "bias2_3_4"
  param { lr_mult: 2 decay_mult: 0 }
  bias_param { filler { type: "constant" value: 0 } channel_shared: false } }
layer { name: "conv2_3_all" type: "Concat" top: "conv2_3_all"
  bottom: "bias2_3_1" bottom: "bias2_3_2" bottom: "bias2_3_3" bottom: "bias2_3_4" }
layer { name: "relu2_3" type: "ReLU" bottom: "conv2_3_all" top: "conv2_3_output" }

layer { name: "pool2" type: "Pooling" bottom: "conv2_3_output" top: "pool2"
  pooling_param { pool: MAX kernel_size: 2 stride: 2 } }


