  param { lr_mult: 1 decay_mult: 0 }
  param { lr_mult: 1 decay_mult: 0 }
  bn_param {
    slope_filler { type: "constant" value: 1 }
    bias_filler { type: "constant" value: 0 } } }
layer { name: "relu3_1_before" type: "ReLU" bottom: "conv3_1_bn" top: "conv3_1_bn" }
layer { name: "bias3_1_1" type: "Bias" bottom: "conv3_1_bn" top: "bias3_1_1"
  param { lr_mult: 2 decay_mult: 0 }
  bias_param { filler { type: "constant" value: 1 } channel_shared: false } }
layer { name: "bias3_1_2" type: "Bias" bottom: "conv3_1_bn" top: "bias3_1_2"
  param { lr_mult: 2 decay_mult: 0 }
  bias_param { filler { type: "constant" value: 0.5 } channel_shared: false } }
layer { name: "bias3_1_3" type: "Bias" bottom: "conv3_1_bn" top: "bias3_1_3"
  param { lr_mult: 2 decay_mult: 0 }
  bias_param { filler { type: "constant" value: -0.5 } channel_shared: false } }  
layer { name: "bias3_1_4" type: "Bias" bottom: "conv3_1_bn" top: "bias3_1_4"
  param { lr_mult: 2 decay_mult: 0 }
  bias_param { filler { type: "constant" value: -1 } channel_shared: false } }
layer { name: "conv3_1_all" type: "Concat" top: "conv3_1_all"
  bottom: "bias3_1_1" bottom: "bias3_1_2" bottom: "bias3_1_3" bottom: "bias3_1_4" }
layer { name: "relu3_1" type: "ReLU" bottom: "conv3_1_all" top: "conv3_1_output" }
#layer { name: "drop_3_1" type: "Dropout" bottom: "conv3_1_output" top: "conv3_1_output"
#  dropout_param { dropout_ratio: 0.4 } }

# ============= conv3-2 bias module =============
layer { name: "conv3_2" type: "Convolution" bottom: "conv3_1_output" top: "conv3_2"
  param { lr_mult: 1 decay_mult: 1 }
  param { lr_mult: 2 decay_mult: 0 }
  convolution_param { num_output: 256 kernel_size: 3 stride: 1 pad: 1
    weight_filler { type: "gaussian" std: 0.05 }
    bias_filler { type: "constant" value: 0 } } }
layer { name: "conv3_2_bn" type: "BN" bottom: "conv3_2" top: "conv3_2_bn"
  param { lr_mult: 1 decay_mult: 0 }
  param { lr_mult: 1 decay_mult: 0 }
  bn_param {
    slope_filler { type: "constant" value: 1 }
    bias_filler { type: "constant" value: 0 } } }
layer { name: "relu3_2_before" type: "ReLU" bottom: "conv3_2_bn" top: "conv3_2_bn" }
layer { name: "bias3_2_1" type: "Bias" bottom: "conv3_2_bn" top: "bias3_2_1"
  param { lr_mult: 2 decay_mult: 0 }
  bias_param { filler { type: "constant" value: 0.3 } channel_shared: false } }
layer { name: "bias3_2_2" type: "Bias" bottom: "conv3_2_bn" top: "bias3_2_2"
  param { lr_mult: 2 decay_mult: 0 }
  bias_param { filler { type: "constant" value: 0.1 } channel_shared: false } }
layer { name: "bias3_2_3" type: "Bias" bottom: "conv3_2_bn" top: "bias3_2_3"
  param { lr_mult: 2 decay_mult: 0 }
  bias_param { filler { type: "constant" value: -0.1 } channel_shared: false } }  
layer { name: "bias3_2_4" type: "Bias" bottom: "conv3_2_bn" top: "bias3_2_4"
  param { lr_mult: 2 decay_mult: 0 }
  bias_param { filler { type: "constant" value: -0.3 } channel_shared: false } }
layer { name: "conv3_2_all" type: "Concat" top: "conv3_2_all"
  bottom: "bias3_2_1" bottom: "bias3_2_2" bottom: "bias3_2_3" bottom: "bias3_2_4" }
layer { name: "relu3_2" type: "ReLU" bottom: "conv3_2_all" top: "conv3_2_output" }
#layer { name: "drop_3_2" type: "Dropout" bottom: "conv3_2_output" top: "conv3_2_output"
#  dropout_param { dropout_ratio: 0.4 } }

# ============= conv3-3 bias module =============
layer { name: "conv3_3" type: "Convolution" bottom: "conv3_2_output" top: "conv3_3"
  param { lr_mult: 1 decay_mult: 1 }
  param { lr_mult: 2 decay_mult: 0 }
  convolution_param { num_output: 512 kernel_size: 3 stride: 1 pad: 1
    weight_filler { type: "gaussian" std: 0.05 }
    bias_filler { type: "constant" value: 0 } } }
layer { name: "conv3_3_bn" type: "BN" bottom: "conv3_3" top: "conv3_3_bn"
  param { lr_mult: 1 decay_mult: 0 }
  param { lr_mult: 1 decay_mult: 0 }
  bn_param {
    slope_filler { type: "constant" value: 1 }
    bias_filler { type: "constant" value: 0 } } }
layer { name: "relu3_3_before" type: "ReLU" bottom: "conv3_3_bn" top: "conv3_3_bn" }
layer { name: "bias3_3_1" type: "Bias" bottom: "conv3_3_bn" top: "bias3_3_1"
  param { lr_mult: 2 decay_mult: 0 }
  bias_param { filler { type: "constant" value: 0 } channel_shared: false } }
layer { name: "bias3_3_2" type: "Bias" bottom: "conv3_3_bn" top: "bias3_3_2"
  param { lr_mult: 2 decay_mult: 0 }
  bias_param { filler { type: "constant" value: 0 } channel_shared: false } }
layer { name: "bias3_3_3" type: "Bias" bottom: "conv3_3_bn" top: "bias3_3_3"
  param { lr_mult: 2 decay_mult: 0 }
  bias_param { filler { type: "constant" value: 0 } channel_shared: false } }  
layer { name: "bias3_3_4" type: "Bias" bottom: "conv3_3_bn" top: "bias3_3_4"
  param { lr_mult: 2 decay_mult: 0 }
  bias_param { filler { type: "constant" value: 0 } channel_shared: false } }
layer { name: "conv3_3_all" type: "Concat" top: "conv3_3_all"
  bottom: "bias3_3_1" bottom: "bias3_3_2" bottom: "bias3_3_3" bottom: "bias3_3_4" }
