  param { lr_mult: 1 decay_mult: 0 }
  param { lr_mult: 1 decay_mult: 0 }
  bn_param {
    slope_filler { type: "constant" value: 1 }
    bias_filler { type: "constant" value: 0 } } }
layer { name: "relu2_1_before" type: "ReLU" bottom: "conv2_1_bn" top: "conv2_1_bn" }
layer { name: "bias2_1_1" type: "Bias" bottom: "conv2_1_bn" top: "bias2_1_1"
  param { lr_mult: 2 decay_mult: 0 }
  bias_param { filler { type: "constant" value: 1 } channel_shared: false } }
layer { name: "bias2_1_2" type: "Bias" bottom: "conv2_1_bn" top: "bias2_1_2"
  param { lr_mult: 2 decay_mult: 0 }
  bias_param { filler { type: "constant" value: 0.5 } channel_shared: false } }
layer { name: "bias2_1_3" type: "Bias" bottom: "conv2_1_bn" top: "bias2_1_3"
  param { lr_mult: 2 decay_mult: 0 }
  bias_param { filler { type: "constant" value: -0.5 } channel_shared: false } }  
layer { name: "bias2_1_4" type: "Bias" bottom: "conv2_1_bn" top: "bias2_1_4"
  param { lr_mult: 2 decay_mult: 0 }
  bias_param { filler { type: "constant" value: -1 } channel_shared: false } }
layer { name: "conv2_1_all" type: "Concat" top: "conv2_1_all"
  bottom: "bias2_1_1" bottom: "bias2_1_2" bottom: "bias2_1_3" bottom: "bias2_1_4" }
layer { name: "relu2_1" type: "ReLU" bottom: "conv2_1_all" top: "conv2_1_output" }

# ============= conv2-2 bias module =============
layer { name: "conv2_2" type: "Convolution" bottom: "conv2_1_output" top: "conv2_2"
  param { lr_mult: 1 decay_mult: 1 }
  param { lr_mult: 2 decay_mult: 0 }
  convolution_param { num_output: 128 kernel_size: 3 stride: 1 pad: 1
    weight_filler { type: "gaussian" std: 0.05 }
    bias_filler { type: "constant" value: 0 } } }
layer { name: "conv2_2_bn" type: "BN" bottom: "conv2_2" top: "conv2_2_bn"
  param { lr_mult: 1 decay_mult: 0 }
  param { lr_mult: 1 decay_mult: 0 }
  bn_param {
    slope_filler { type: "constant" value: 1 }
    bias_filler { type: "constant" value: 0 } } }
layer { name: "relu2_2_before" type: "ReLU" bottom: "conv2_2_bn" top: "conv2_2_bn" }
layer { name: "bias2_2_1" type: "Bias" bottom: "conv2_2_bn" top: "bias2_2_1"
  param { lr_mult: 2 decay_mult: 0 }
  bias_param { filler { type: "constant" value: 0.3 } channel_shared: false } }
layer { name: "bias2_2_2" type: "Bias" bottom: "conv2_2_bn" top: "bias2_2_2"
  param { lr_mult: 2 decay_mult: 0 }
  bias_param { filler { type: "constant" value: 0.1 } channel_shared: false } }
layer { name: "bias2_2_3" type: "Bias" bottom: "conv2_2_bn" top: "bias2_2_3"
  param { lr_mult: 2 decay_mult: 0 }
  bias_param { filler { type: "constant" value: -0.1 } channel_shared: false } }  
layer { name: "bias2_2_4" type: "Bias" bottom: "conv2_2_bn" top: "bias2_2_4"
  param { lr_mult: 2 decay_mult: 0 }
  bias_param { filler { type: "constant" value: -0.3 } channel_shared: false } }
layer { name: "conv2_2_all" type: "Concat" top: "conv2_2_all"
  bottom: "bias2_2_1" bottom: "bias2_2_2" bottom: "bias2_2_3" bottom: "bias2_2_4" }
layer { name: "relu2_2" type: "ReLU" bottom: "conv2_2_all" top: "conv2_2_output" }

