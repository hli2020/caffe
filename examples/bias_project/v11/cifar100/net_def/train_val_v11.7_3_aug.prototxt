name: "CIFAR100_VGGNet_Conv_96_128_256_FC2048"
##################################### data #####################################
layer { 
  name: "data" type: "Data" top: "data" top: "label"
  data_param { backend: LMDB batch_size: 50 shuffle_pool_size: 500
    #source: "/home/hyli/dataset/cifar/cifar100_train_lmdb" }
    #source: "/media/hongyang/research_at_large/Q-dataset/cifar/cifar100_train_lmdb" }
    source: "/data2/dataset/cifar/cifar100_train_lmdb" }
  transform_param { 
    mirror: true
    customize: true
    crop_size: 32
    resize_min: 32
    resize_max: 40
    random_crop: true
    mean_file: "/data2/dataset/cifar/cifar10_mean.binaryproto" } 
    #mean_file: "/home/hyli/dataset/cifar/cifar10_mean.binaryproto" } 
    #mean_file: "/media/hongyang/research_at_large/Q-dataset/cifar/cifar10_mean.binaryproto" }
  include: { phase: TRAIN } 
}

layer { 
  name: "data" type: "Data" top: "data" top: "label"
  data_param { backend: LMDB batch_size: 50
    source: "/data2/dataset/cifar/cifar100_test_lmdb" }
    #source: "/home/hyli/dataset/cifar/cifar100_test_lmdb" }
    #source: "/media/hongyang/research_at_large/Q-dataset/cifar/cifar100_test_lmdb" }
  transform_param { 
    mirror: false
    customize: true
    crop_size: 32
    resize_min: 32
    resize_max: 40
    random_crop: false
    mean_file: "/data2/dataset/cifar/cifar10_mean.binaryproto" } 
    #mean_file: "/home/hyli/dataset/cifar/cifar10_mean.binaryproto" } 
    #mean_file: "/media/hongyang/research_at_large/Q-dataset/cifar/cifar10_mean.binaryproto" }
  include: { phase: TEST } 
}

# ============= conv1-1 bias module =============
layer { name: "conv1_1" type: "Convolution" bottom: "data" top: "conv1_1"
  param { lr_mult: 1 decay_mult: 1 }
  param { lr_mult: 2 decay_mult: 0 }
  convolution_param { num_output: 96 kernel_size: 3 stride: 1 pad: 1
    weight_filler { type: "gaussian" std: 0.05 }
    bias_filler { type: "constant" value: 0 } } }
layer { name: "conv1_1_bn" type: "BN" bottom: "conv1_1" top: "conv1_1_bn"
  param { lr_mult: 1 decay_mult: 0 }
  param { lr_mult: 1 decay_mult: 0 }
  bn_param {
    slope_filler { type: "constant" value: 1 }
    bias_filler { type: "constant" value: 0 } } }
layer { name: "relu1_1_before" type: "ReLU" bottom: "conv1_1_bn" top: "conv1_1_bn" }
layer { name: "bias1_1_1" type: "Bias" bottom: "conv1_1_bn" top: "bias1_1_1"
  param { lr_mult: 2 decay_mult: 0 }
  bias_param { filler { type: "constant" value: 1 } channel_shared: false } }
layer { name: "bias1_1_2" type: "Bias" bottom: "conv1_1_bn" top: "bias1_1_2"
  param { lr_mult: 2 decay_mult: 0 }
  bias_param { filler { type: "constant" value: 0.5 } channel_shared: false } }
layer { name: "bias1_1_3" type: "Bias" bottom: "conv1_1_bn" top: "bias1_1_3"
  param { lr_mult: 2 decay_mult: 0 }
  bias_param { filler { type: "constant" value: -0.5 } channel_shared: false } }  
layer { name: "bias1_1_4" type: "Bias" bottom: "conv1_1_bn" top: "bias1_1_4"
  param { lr_mult: 2 decay_mult: 0 }
  bias_param { filler { type: "constant" value: -1 } channel_shared: false } }
layer { name: "conv1_1_all" type: "Concat" top: "conv1_1_all"
  bottom: "bias1_1_1" bottom: "bias1_1_2" bottom: "bias1_1_3" bottom: "bias1_1_4" }
layer { name: "relu1_1" type: "ReLU" bottom: "conv1_1_all" top: "conv1_1_output" }

# ============= conv1-2 bias module =============
layer { name: "conv1_2" type: "Convolution" bottom: "conv1_1_output" top: "conv1_2"
  param { lr_mult: 1 decay_mult: 1 }
  param { lr_mult: 2 decay_mult: 0 }
  convolution_param { num_output: 96 kernel_size: 3 stride: 1 pad: 1
    weight_filler { type: "gaussian" std: 0.05 }
    bias_filler { type: "constant" value: 0 } } }
layer { name: "conv1_2_bn" type: "BN" bottom: "conv1_2" top: "conv1_2_bn"
  param { lr_mult: 1 decay_mult: 0 }
  param { lr_mult: 1 decay_mult: 0 }
  bn_param {
    slope_filler { type: "constant" value: 1 }
    bias_filler { type: "constant" value: 0 } } }
layer { name: "relu1_2_before" type: "ReLU" bottom: "conv1_2_bn" top: "conv1_2_bn" }
layer { name: "bias1_2_1" type: "Bias" bottom: "conv1_2_bn" top: "bias1_2_1"
  param { lr_mult: 2 decay_mult: 0 }
  bias_param { filler { type: "constant" value: 0.3 } channel_shared: false } }
layer { name: "bias1_2_2" type: "Bias" bottom: "conv1_2_bn" top: "bias1_2_2"
  param { lr_mult: 2 decay_mult: 0 }
  bias_param { filler { type: "constant" value: 0.1 } channel_shared: false } }
layer { name: "bias1_2_3" type: "Bias" bottom: "conv1_2_bn" top: "bias1_2_3"
  param { lr_mult: 2 decay_mult: 0 }
  bias_param { filler { type: "constant" value: -0.1 } channel_shared: false } }  
layer { name: "bias1_2_4" type: "Bias" bottom: "conv1_2_bn" top: "bias1_2_4"
  param { lr_mult: 2 decay_mult: 0 }
  bias_param { filler { type: "constant" value: -0.3 } channel_shared: false } }
layer { name: "conv1_2_all" type: "Concat" top: "conv1_2_all"
  bottom: "bias1_2_1" bottom: "bias1_2_2" bottom: "bias1_2_3" bottom: "bias1_2_4" }
layer { name: "relu1_2" type: "ReLU" bottom: "conv1_2_all" top: "conv1_2_output" }

# ============= conv1-3 bias module =============
layer { name: "conv1_3" type: "Convolution" bottom: "conv1_2_output" top: "conv1_3"
  param { lr_mult: 1 decay_mult: 1 }
  param { lr_mult: 2 decay_mult: 0 }
  convolution_param { num_output: 96 kernel_size: 3 stride: 1 pad: 1
    weight_filler { type: "gaussian" std: 0.05 }
    bias_filler { type: "constant" value: 0 } } }
layer { name: "conv1_3_bn" type: "BN" bottom: "conv1_3" top: "conv1_3_bn"
  param { lr_mult: 1 decay_mult: 0 }
  param { lr_mult: 1 decay_mult: 0 }
  bn_param {
    slope_filler { type: "constant" value: 1 }
    bias_filler { type: "constant" value: 0 } } }
layer { name: "relu1_3_before" type: "ReLU" bottom: "conv1_3_bn" top: "conv1_3_bn" }
layer { name: "bias1_3_1" type: "Bias" bottom: "conv1_3_bn" top: "bias1_3_1"
  param { lr_mult: 2 decay_mult: 0 }
  bias_param { filler { type: "constant" value: 0 } channel_shared: false } }
layer { name: "bias1_3_2" type: "Bias" bottom: "conv1_3_bn" top: "bias1_3_2"
  param { lr_mult: 2 decay_mult: 0 }
  bias_param { filler { type: "constant" value: 0 } channel_shared: false } }
layer { name: "bias1_3_3" type: "Bias" bottom: "conv1_3_bn" top: "bias1_3_3"
  param { lr_mult: 2 decay_mult: 0 }
  bias_param { filler { type: "constant" value: 0 } channel_shared: false } }  
layer { name: "bias1_3_4" type: "Bias" bottom: "conv1_3_bn" top: "bias1_3_4"
  param { lr_mult: 2 decay_mult: 0 }
  bias_param { filler { type: "constant" value: 0 } channel_shared: false } }
layer { name: "conv1_3_all" type: "Concat" top: "conv1_3_all"
  bottom: "bias1_3_1" bottom: "bias1_3_2" bottom: "bias1_3_3" bottom: "bias1_3_4" }
layer { name: "relu1_3" type: "ReLU" bottom: "conv1_3_all" top: "conv1_3_output" }

layer { name: "pool1" type: "Pooling" bottom: "conv1_3_output" top: "pool1"
  pooling_param { pool: MAX kernel_size: 2 stride: 2 } }

# ============= conv2-1 bias module =============
layer { name: "conv2_1" type: "Convolution" bottom: "pool1" top: "conv2_1"
  param { lr_mult: 1 decay_mult: 1 }
  param { lr_mult: 2 decay_mult: 0 }
  convolution_param { num_output: 128 kernel_size: 3 stride: 1 pad: 1
    weight_filler { type: "gaussian" std: 0.05 }
    bias_filler { type: "constant" value: 0 } } }
